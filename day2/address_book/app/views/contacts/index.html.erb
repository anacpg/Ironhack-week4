<p id="notice"><%= notice %></p>

<h1>Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.address %></td>
        <td><%= contact.phone_number %></td>
        <td><%= contact.email_address %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
